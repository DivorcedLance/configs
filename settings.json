{
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.formatOnSave": false,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.rulers": [79],
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  // "html.format.indentInnerHtml": true,
  // "html.format.indentHandlebars": true,
  "liveServer.settings.donotShowInfoMsg": true,
  // "liveServer.settings.CustomBrowser": "firefox",
  "prettier.singleQuote": true,
  "python.formatting.blackArgs": ["--skip-string-normalization"],
  "python.terminal.focusAfterLaunch": true,
  "python.formatting.provider": "autopep8",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "Tokyo Night Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "none",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.formatOnType": true
  },
  "[cpp]": {
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.onTaskErrors": "abort",
  "code-runner.preserveFocus": false,
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++ '$fileName' -o '$fileNameWithoutExt.exe' && & '$dir$fileNameWithoutExt'",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "& \"$pythonPath\" -u $fullFileName",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "cd $dir && dotnet run $fileName",
    "vbscript": "cscript //Nologo",
    "typescript": "ts-node",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket",
    "scheme": "csi -script",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runhaskell",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "lisp": "sbcl --script",
    "kit": "kitc --run",
    "v": "v run",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "sml": "cd $dir && sml $fileName"
  },
  "C_Cpp.default.compilerPath": "C:/msys64/mingw64/bin/g++.exe",
  "C_Cpp.default.intelliSenseMode": "windows-gcc-x64",
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true,
    "cpp": true
  },
  "jupyter.askForKernelRestart": false,
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "workbench.colorCustomizations": {
    "[Tokyo Night Dark]": {
      "editor.lineHighlightBackground": "#0a0a0d",
      // "editor.wordHighlightBackground": "#0a0a0d",
      "editor.wordHighlightBorder": "#a9b1d6",
      "editor.selectionBackground": "#3d59a1aa",
      // "editor.selectionHighlightBackground": "#eb0cd1fb",
      "editor.selectionHighlightBorder": "#e0af68",
    },

    // "editorRuler.foreground": "#0474bc",
    "statusBar.background": "#1A1A1A",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.layoutControl.enabled": false,
  "workbench.editorAssociations": {
    "*.dat": "default",
    "*.db": "sqlite-viewer.option"
  },
  "quokka.showStartViewOnFeatureRelease": false,
  "redhat.telemetry.enabled": true,
  "terminal.integrated.env.windows": {},
  "console-ninja.toolsToEnableSupportAutomaticallyFor": {
    "vite": true,
    "live-server-extension": true,
    "live-preview-extension": true
  },
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "editor.minimap.enabled": false,
  "console-ninja.showWhatsNew": false,
  "workbench.activityBar.location": "hidden",
  "console-ninja.featureSet": "Community",
  "latex-workshop.latex.outDir": "%DIR%/build",
  "spellright.notificationClass": "information",
  "cSpell.enableFiletypes": [
    "css"
  ],
  "cSpell.language": "en,es-ES",
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "standard.engine": "ts-standard",
  "prettier.semi": false,
  "standard.autoFixOnSave": true,
  "eslint.format.enable": true,
  "cSpell.userWords": [
    "agreglo",
    "arbol",
    "Arbol",
    "arduino",
    "Busqueda",
    "cinemómetro",
    "desapilar",
    "Despues",
    "estandar",
    "FISI",
    "fontsource",
    "Inocencio",
    "Iterador",
    "nodisponible",
    "nodoprevio",
    "Numeros",
    "onrender",
    "Palindromo",
    "palíndromo",
    "penultimo",
    "posicion",
    "Preorden",
    "Quispe",
    "raiz",
    "recursivamente",
    "solucionfisi",
    "ssid",
    "sudokuapp",
    "sudokulogic",
    "tamano",
    "tareasemana",
    "UNMSM",
    "vacia",
    "Wokwi",
    "zustand"
  ],
  "window.zoomLevel": 1,
  "gitlens.ai.experimental.provider": "openai",
  "gitlens.ai.experimental.openai.model": "gpt-4-turbo-preview",
  "liveServer.settings.CustomBrowser": "chrome",
  "liveServer.settings.port": 5001,
}
